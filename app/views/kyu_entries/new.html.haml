.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", default: "List")}", kyu_entries_path
      %li.active= link_to "#{t("web-app-theme.new", default: "New")}", new_kyu_entry_path
  .content
    %h2.title
      = t("web-app-theme.new", default: "New")
      Post
    .inner
      = form_for @kyu_entry, url: kyu_entries_path, html: { class: :form, id: "formID1", multipart: true } do |f|
        = render 'form', f: f, kyu_entry: @kyu_entry, attachment: @attachment
    #default_preview
- content_for :sidebar, render(partial: 'sidebar', locals: {tag_cloud_hash: @tag_cloud_hash, users: @users})